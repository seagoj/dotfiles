# Example Configuration
#
# Host domain.com
#     User root
#     Port 22
#     IdentityFile ~/.ssh/id_rsa # path to ssh  key
#     IdentitiesOnly Yes # Only attempt to login via ssh key
#     PubkeyAuthentication No # Bypass attempting SSH key authentication
##

Host margaretandjeremy.com
    HostName 162.243.83.115
    User root

Host development
    HostName 192.168.0.7
    User root

Host staging
    HostName 192.168.0.47
    User root

Host production
    HostName 192.168.0.48
    User root

Host synology
    HostName home.seagoj.com
    Port 18450
    IdentityFile ~/.ssh/id_rsa
    User admin
