#!/usr/bin/zsh

rm -r ~/.vim/bundle/YouCOmpleteMe/build_cygwin
mkdir ~/.vim/bundle/YouCompleteMe/build_cygwin && cd ~/.vim/bundle/YouCompleteMe/build_cygwin
cmake -D CMAKE_CXX_COMPILER=/usr/bin/gcc -G "Unix Makefiles" . ../cpp
make ycm_core
