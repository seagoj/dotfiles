function notYetImplemented() {
    echo "$OS_TYPE is not yet implemented for $1."
    exit 1
}

function clearFunc() {
    if type $1 | grep 'function' > /dev/null; then
        unset -f $1
    fi
}

if ! which $1 > /dev/null; then
    case $OS_TYPE in
    Arch)
        if type installArch | grep 'function' > /dev/null; then
            installArch
        else
            notYetImplemented $1
        fi
        ;;
    Darwin | Mac)
        if type installMac | grep 'function' > /dev/null; then
            installMac
        elif type installDarwin | grep 'function' > /dev/null; then
            installDarwin
        else
            notYetImplemented $1
        fi
        ;;
    *)
        notYetImplemented $1
        ;;
    esac
fi

clearFunc installArch
clearFunc installDarwin
clearFunc installMac
